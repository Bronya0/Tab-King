{
  "manifest_version": 3,
  "name": "Tab King",
  "version": "1.1.0",
  "description": "A beautiful custom new tab page with search functionality",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [],
  "host_permissions": [
    "https://suggestqueries.google.com/*",
    "https://api.bing.com/*",
    "https://suggestion.baidu.com/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "icons": {
    "16": "favicon.ico",
    "48": "favicon.ico",
    "128": "favicon.ico"
  }
}